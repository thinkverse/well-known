{
    "name": "thinkverse/well-known",
    "description": "A Laravel package for quickly adding .well-known URLs",
    "keywords": [
        "thinkverse",
        "laravel",
        "well-known",
        "laravel-package",
        "pacakge"
    ],
    "homepage": "https://github.com/thinkverse/well-known",
    "license": "MIT",
    "authors": [
        {
            "name": "Kim Hallberg",
            "email": "hallberg.kim@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Thinkverse\\WellKnown\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thinkverse\\WellKnown\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thinkverse\\WellKnown\\WellKnownServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
