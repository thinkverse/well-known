{
    "name": "thinkverse/well-known",
    "description": "A Laravel package for quickly adding .well-known URLs",
    "keywords": [
        "thinkverse",
        "laravel",
        "well-known",
        "laravel-package",
        "pacakge"
    ],
    "homepage": "https://github.com/thinkverse/well-known",
    "license": "MIT",
    "authors": [
        {
            "name": "Kim Hallberg",
            "email": "hallberg.kim@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Thinkverse\\WellKnown\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thinkverse\\WellKnown\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thinkverse\\WellKnown\\WellKnownServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
